{
    "random_seed": 42,
    "sampler": {
        "name": "SamplerSqrt",
        "params": {
            "digits": 10,
            "intermediate_steps": "scratchpad"
        }
    },
    "model_params": {
        "n_positions": 1500,
        "n_embd": 384,
        "n_layer": 6,
        "n_head": 6
    },
    "dataset_params": {
        "train_dataset_size": 1000,
        "val_dataset_size": 10,
        "test_dataset_size": 1000
    },
    "training_params": {
        "batch_size": 8,
        "max_epochs": 200,
        "logging_steps": 100,
        "logging_compute_accuracy": false,
        "early_stopping_patience": 5,
        "early_stopping_threshold": 0.0001
    }
}
